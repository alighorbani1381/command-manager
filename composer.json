{
  "name": "alighorbani1381/command-manager",
  "description": "Automatically run your artisan commands",
  "keywords": [
    "laravel-command",
    "laravel-automatic-deploy"
  ],
  "homepage": "https://github.com/alighorbani1381/command-manager",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ali",
      "email": "alighorbani20002@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "8.0.*|8.1.*|8.2.*",
    "laravel/framework": "8.*|9.*|10.*"
  },
  "autoload": {
    "psr-4": {
      "Alighorbani\\CommandManager\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alighorbani\\CommandManager\\CommandManagerServiceProvider"
      ]
    }
  }
}